<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='StartPoint'>

    <entry-point class='com.netcracker.shop.client.StartPoint'/>
    <inherits name='com.google.gwt.user.User' />
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="org.fusesource.restygwt.RestyGWT"/>
    <replace-with class="com.netcracker.shop.client.ClientFactoryImpl">
        <when-type-is class="com.netcracker.shop.client.ClientFactory" />
    </replace-with>
    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>
    <source path='server'/>
    <source path='gin'/>
    <!--GWT Material-->
    <inherits name="gwt.material.design.GwtMaterialBasicWithJQuery"/>
    <inherits name="gwt.material.design.addins.GwtMaterialAddins"/>
    <inherits name="gwt.material.design.GwtMaterialTable"/>
    <inherits name="gwt.material.design.themes.ThemeBlue"/>
    <inherits name="com.google.gwt.inject.Inject"/>
</module>
