<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>


    <m:MaterialPanel ui:field="panel">
        <m:MaterialColumn grid="s12 m8 l6" offset="l4" >
        <m:MaterialTextBox ui:field="login" type="TEXT" placeholder="login"/>
        <m:MaterialTextBox ui:field="password" type="PASSWORD" placeholder="password"/>
        <m:MaterialRow>
            <m:MaterialCheckBox text="Keep me"/>
            <m:MaterialButton ui:field="LOG_IN" text="LOG IN"/>
        </m:MaterialRow>
        </m:MaterialColumn>
    </m:MaterialPanel>
</ui:UiBinder>