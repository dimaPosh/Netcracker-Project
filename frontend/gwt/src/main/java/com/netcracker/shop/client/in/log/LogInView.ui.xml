<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <m:MaterialPanel>
        <m:MaterialColumn grid="s12 m8 l6" offset="l4" >
        <m:MaterialTextBox type="TEXT" placeholder="login"/>
        <m:MaterialTextBox type="PASSWORD" placeholder="password"/>
        <m:MaterialRow>
            <m:MaterialButton ui:field="button" text="LOG IN"/>
        </m:MaterialRow>
        <m:MaterialRow>
            <m:MaterialCheckBox text="Keep me"/>
        </m:MaterialRow>
        </m:MaterialColumn>
    </m:MaterialPanel>
</ui:UiBinder>