<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <b:Container>
            <b:PageHeader>Yay buttons!</b:PageHeader>
            <b:Button>Some button</b:Button>
            <b:Button type="DANGER" size="LARGE">Dangerous button</b:Button>
        </b:Container>
        <b:TextBox ui:field="input"/>
        <g:Button ui:field="calculate">Calculate</g:Button>
        <g:Label ui:field="output"/>
    </g:HTMLPanel>
</ui:UiBinder>